{
  "name": "react-native-sunshine-app",
  "displayName": "react-native-sunshine-app",
  "expo": {
    "name": "Sunshine",
    "slug": "sunshine",
    "version": "1.0.0",
    "assetBundlePatterns": [
      "**/*"
    ],
    "icon": "https://github.com/udacity/ud851-Sunshine/blob/S05.01-Exercise-AsyncTaskLoader/app/src/main/res/mipmap-mdpi/ic_launcher.png?raw=true",
    "splash": {
      "image": "https://github.com/expo/expo/blob/master/templates/expo-template-blank/assets/splash.png?raw=true",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "infoPlist": {
        "UIBackgroundModes": [
          "fetch"
        ]
      }
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./local/path/to/myNotificationIcon.png",
          "mode": "production"
        }
      ]
    ]
  }

}
